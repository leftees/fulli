$(document)
  .trigger('table:reload', ['<%= j render("organisations/#{params[:display]}", obj: @search) %>', '<%= params[:display] %>'])
  .trigger('filter:toggle', '<%= params[:display] %>')
<% if params[:display] == 'graph' %>
  .trigger('filter:hide')
<% else %>
  .trigger('filter:show')
<% end %>
